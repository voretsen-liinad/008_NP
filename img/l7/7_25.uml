actor "Пользователь" as user


interface "HTTPS" as i_https
interface "MTProto" as i_mtproto


[Интерфейс Telegram] as ui

[API сервер] as srv_api

package "Реализуемая система" {

[Бизнес-логика бота] as bl

interface "Websocket" as i_ws_d1
interface "Websocket" as i_ws_d2

[Драйвер] as drv_1
[Драйвер] as drv_2

note bottom of drv_2
Отладочный драйвер,
выводящий команды на консоль.
end note

bl --( i_ws_d1
bl --( i_ws_d2

drv_1 --( i_ws_d1
drv_2 --( i_ws_d2

}

interface "Последовательный порт" as i_serial

rectangle "Манипулятор" <<$roboticArm>> as device
rectangle "Консоль" <<$screen>> as console

device --( i_serial
drv_1 --( i_serial

drv_2 --> console

bl --( i_https
srv_api ---( i_https

srv_api --( i_mtproto
ui --( i_mtproto

user <--> ui
